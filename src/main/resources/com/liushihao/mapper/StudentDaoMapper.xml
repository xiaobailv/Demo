<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.liushihao.dao.StudentDao">

    <select id="findAll" resultType="com.liushihao.entity.Student">
        SELECT
            id,
            NAME,
            age,
            grade,
            major,
            school
        FROM
            student
    </select>
</mapper>